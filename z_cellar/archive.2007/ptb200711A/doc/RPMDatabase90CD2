libc.so.6(GLIBC_2.1)  
libc.so.6(GLIBC_2.1.3)  
libgcc_s.so.1  
libm.so.6  
libpthread.so.0  
libqt-mt.so.3  
libstdc++.so.5  
python >= 2.2.2
rpmlib(CompressedFileNames) <= 3.0.4-1
rpmlib(PayloadFilesHavePrefix) <= 4.0-1
-----------------------------------------------------------------------------
PROVIDES
-----------------------------------------------------------------------------
libsip.so.9  
sip = 3.5-2

